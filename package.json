{
  "name": "auto-changelog",
  "displayName": "Auto CHANGELOG.MD",
  "description": "An extension to vscode that generates changelogs from your commit messages",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "publisher": "rudy-tools-box",
  "repository": {
    "type": "git",
    "url": "https://github.com/DioLps/auto-changelog.git"
  },
  "icon": ".github/icon.png",
  "license": "Apache-2.0",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "auto-changelog.commit",
        "title": "Commit and Generate CHANGELOG.MD"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.54.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "fs-extra": "^11.3.0"
  }
}
